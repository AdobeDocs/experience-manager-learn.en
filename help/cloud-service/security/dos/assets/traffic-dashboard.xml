          <query>index=$index$ sourcetype=$sourcetype$ url=$url$ host=$host$ req_ua="$req_ua$" $statusSelection$ cache IN ($cache$) cli_ip="$cli_ip$"
| rex field=url "((?&lt;urlPath&gt;[^?]*)[\?]?(?&lt;urlQs&gt;.*))"
| stats count dc(urlQs) by method host urlPath
| sort -count</query>
